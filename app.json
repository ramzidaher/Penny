{
  "expo": {
    "name": "Penny",
    "slug": "penny",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Penny Logo RD.png",
    "userInterfaceStyle": "automatic",
    "scheme": "penny",
    "main": "expo-router/entry",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#080808"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pennyfinance.app",
      "buildNumber": "2",
      "associatedDomains": [],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Penny Logo RD.png",
        "backgroundColor": "#080808"
      },
      "package": "com.pennyfinance.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "penny",
              "host": "truelayer-callback"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/Penny Logo RD.png",
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#F8F8F8"
        }
      ],
      "@react-native-community/datetimepicker",
      "expo-sqlite",
      "expo-local-authentication"
    ],
    "extra": {
      "eas": {
        "projectId": "ddf1d704-a3b6-4f48-85c3-038424c2a2c9"
      }
    }
  }
}
